{
  "name": "plainsdk",
  "version": "0.1.0",
  "description": "Generate idiomatic SDKs from OpenAPI specifications",
  "main": "dist/index.js",
  "bin": {
    "plainsdk": "bin/plainsdk.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build"
  },
  "keywords": [
    "sdk",
    "openapi",
    "swagger",
    "generator",
    "api",
    "typescript",
    "python",
    "rest"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^5.57.0",
    "acorn": "^8.8.2",
    "ast-types": "^0.14.2",
    "axios": "^1.8.3",
    "chalk": "^4.1.2",
    "commander": "^9.5.0",
    "diff": "^5.1.0",
    "inquirer": "^8.2.5",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.9",
    "openapi-types": "^12.1.0",
    "ora": "^5.4.1",
    "prettier": "^2.8.7"
  },
  "devDependencies": {
    "@types/diff": "^5.0.3",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ]
}